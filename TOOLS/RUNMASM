AMOV SP,1000MOV DX,190MOV BX,100MOV AH,4AINT 21JC 180MOV BX,2000MOV AH,48INT 21JC 180MOV ES,AXMOV BX,FFFFMOV AH,48INT 21MOV AH,48INT 21JC 180MOV AH,49INT 21JC  180PUSH CSPOP ESXOR AX,AXPUSH AXPUSH AXPUSH AXPUSH AXPUSH CSMOV BX,80PUSH BXPUSH AXMOV BX,SPMOV DX,1A0MOV AX,4B00INT 21MOV BX,CSMOV SS,BXMOV SP,1000MOV DX,1B0JC 180MOV AH,4DINT 21MOV AH,4CINT 21A180MOV AH,9INT 21MOV AX,4C01INT 21E190 'Out of memory$'E1A0 'MASM.EXE' 0E1B0 'Exec failed$'RCX100WQ